<?xml version="1.0" encoding="utf-8"?>
<mx:Panel 
	xmlns:mx="http://www.adobe.com/2006/mxml" 
	xmlns:pm="ac.iocsample.passtheparcel.model.pm.*"
	layout="absolute"  
	borderColor="#ff9933"
	title="{ endPM.name }" 
	creationComplete="init()">
	<mx:Script>
		<![CDATA[
			import ac.iocsample.prana.factory.PranaFactory;
			import org.pranaframework.context.support.XMLApplicationContext;
			import ac.iocsample.passtheparcel.model.pm.EndPM;
			
			[Bindable]
			public var endPM : EndPM;	
			
			private function init() : void
		    {
				endPM = PranaFactory.getObject("endPM");
		    }
	
		]]>
	</mx:Script>
	<mx:Label text="{ endPM.parcel.contents }" />
</mx:Panel>