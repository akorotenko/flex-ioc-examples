<?xml version="1.0" encoding="utf-8"?>
<objects 
	xmlns="http://www.pranaframework.org/objects" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.pranaframework.org/objects http://www.pranaframework.org/schema/objects/prana-objects-0.6.xsd">
	
	<!--  detached pms  -->
 	<object 
 		id="endPM" 
  		class="ac.iocsample.passtheparcel.model.pm.EndPM">
  		<constructor-arg ref="parcel"/>
  	</object>
  	
  	<!-- hierarchical pms -->
  	<object 
 		id="startPMHierarchy" 
  		class="ac.iocsample.passtheparcel_hierarchy.model.pm.StartPMHierarchy"/>
	

	<!--  domains  -->
	<object 
		id="parcel"
		class="ac.iocsample.passtheparcel.model.domain.Parcel">
    	<constructor-arg value="I'm the prana parcel"/>
    </object>

	<!-- populate the domain -->
 	<object 
 		id="otherPM" 
  		class="ac.iocsample.populatedomain.model.pm.OtherPM">
  		<constructor-arg ref="account"/>
  	</object>

 	<object 
 		id="mainPM" 
  		class="ac.iocsample.populatedomain.model.pm.MainPM">
  		<constructor-arg ref="account"/>
  	</object>
	
	<object 
		id="account"
		class="ac.iocsample.populatedomain.model.domain.Account"/>
    	
	
	
	
</objects>
